{
    "defaultSeverity": "error",
    "jsRules": {
        "no-empty": true
    },
    "rules": {
        "import-blacklist": [true, "rxjs/Rx"],
        "import-spacing": true,
        "jsdoc-format": true,
        "no-redundant-jsdoc": true,
        "no-reference-import": true,
        "one-line": [
            true,
            "check-catch",
            "check-else",
            "check-finally",
            "check-open-brace",
            "check-whitespace"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type",
            "check-typecast"
        ]
    },
    "rulesDirectory": []
}
